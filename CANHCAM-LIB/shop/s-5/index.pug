section.canhcam-shop-5
	article.product-lists.container
		span.fas.fa-sliders-h.pl-filter-trigger
		.pl-filter
			h4.pl-filter-title Danh mục sản phẩm
			.pl-filter-block
				h4 Tìm kiếm
			span.lnr.lnr-cross.pl-close
		.pl-content
			.row
				.col-12
					.pl-tab-filter-wrapper
						.title
							h2 Vali 1
							p Tìm thấy 60 sản phẩm
						select.custom-select
							option(value="") ALL
							option(value="") Sản phầm 1
							option(value="") Sản phẩm 2
			.row
				.col-6.col-md-3.item
					figure
						.pl-new
							span.badge.badge-secondary New
						.pl-boximg
							img.img-fluid(src="img/product/1.jpg", alt="alt")
						.pl-hidden
							a.btn.btn-more(href="#") Chi tiết
							a.btn.btn-buy(href="#") Mua sản phẩm
						figcaption
							ul
								li
									a.color(href="#" style="background: #2e3192")
								li
									a.color(href="#" style="background: #ec8b00")
								li
									a.color(href="#" style="background: #659f13")
							hgroup
								h3.brand
									a(href="#") Kamiliant
								h2.name
									a(href="#") Onda Size 79
								h3.type Spinner 79
							.pl-cost
								span.new 2.450.000đ
								span.old 4.900.000đ
